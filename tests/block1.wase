include lib/runtime;

export memory 1;

export _start() -> () {
	block {
		i = 1;
		loop {
			printi32(i);
			printByte(10);
			i := i + 1;
			// This breaks out of the upper loop
			break_if<1>(i > 10);
			// This is really continue inside this loop
			break<>();
		};
		// Never reached "E"
		printByte(69);
		{}
	};
	// This is "F"
	printByte(70);
}
